<?xml version="1.0" encoding="UTF-8"?>
<api context="/5_1_1_Routing_messages_based_on_content_of_message_test" name="5_1_1_Routing_messages_based_on_content_of_message_test" xmlns="http://ws.apache.org/ns/synapse">
    <resource methods="POST">
        <inSequence>
            <property expression="$body//m:StockName" name="StockName" scope="default" type="STRING" xmlns:m="http://www.example.org/stock"/>
            <switch source="get-property('StockName')">
                <case regex="IBM">
                    <call>
                        <endpoint key="05_EP_XML"/>
                    </call>
                </case>
                <default/>
            </switch>
            <respond description="PASS"/>
        </inSequence>
        <outSequence/>
        <faultSequence/>
    </resource>
</api>
